<h1>Sign In</h1>

<div>
<%= form_with url: login_path, data: { turbo: false } do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit "Sign In" %>
<% end %>
</div>