
<table class="table">
<thead>
  <tr>
    <th scope="col">ID</th>
    <th scope="col">Name</th>
    <th scope="col">Email</th>
    <th scope="col">IsAdmin</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
    <tr id="<%= dom_id user %>">
      <th><%= user.id %></th>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.is_admin %></td>
    </tr>
  <% end %>
</tbody>
</table>